<div id="<%= dom_id exercise_tracking %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= exercise_tracking.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Exercise:</strong>
    <%= exercise_tracking.exercise_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Reps:</strong>
    <%= exercise_tracking.reps %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Weight:</strong>
    <%= exercise_tracking.weight %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Performed at:</strong>
    <%= exercise_tracking.performed_at %>
  </div>
</div>
