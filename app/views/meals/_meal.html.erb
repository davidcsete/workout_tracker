<div id="<%= dom_id meal %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= meal.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Meal type:</strong>
    <%= meal.meal_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Consumed at:</strong>
    <%= meal.consumed_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= meal.user_id %>
  </div>
</div>
